<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginsharelist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginShareList">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateTaskUser" />
        <activity android:name=".TaskMenuUser" />
        <activity android:name=".AccountInfo" />
        <activity android:name=".ForgetPassword" />
        <activity android:name=".CreateTaskAdmin" />
        <activity android:name=".RegisterUser" />
        <activity android:name=".GroupSearch" />
        <activity android:name=".AutoCompleteGroupSearch" />
        <activity android:name=".CreateGroup" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>